from typing import List

import numpy as np

class NNDescent:
    def __init__(
        self, metric: str, n_neighbors: int, pruning_degree_multiplier: float, pruning_prob: float, leaf_size: int
    ) -> None: ...
    def fit(self, input: np.ndarray) -> None: ...
    def query(self, query_vec: np.ndarray, k: int, epsilon: float = 0.1) -> List[int]: ...
    def batch_query(self, query_vecs: np.ndarray, k: int, epsilon: float = 0.1) -> List[List[int]]: ...

class ostream_redirect:
    def __init__(self, stdout: bool = True, stderr: bool = True) -> None: ...
    def __enter__(self) -> None: ...
    def __exit__(self, exc_type, exc_value, traceback) -> None: ...
